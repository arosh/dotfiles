.ANSI-01;34-brblue 01;34
.ANSI-01;36-brcyan 01;36
.ANSI-37-white 37
.ANSI-01;37-brwhite 01;37
.ANSI-33-yellow 33
.ANSI-01;31-brred 01;31
.ANSI-31-red 31
.ANSI-35-magenta 35
.ANSI-01;35-brmagenta 01;35
